{
    "name": "hametuha/rest-pattern",
    "description": "Rest API Patten class for easy create rest-api based WordPress",
    "type": "wordpress-library",
    "license": "GPL-3.0-or-later",
    "scripts": {
        "test": "phpunit"
    },
    "authors": [
        {
            "name": "Hametuha",
            "email": "info@hametuha.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Hametuha\\RestPattern": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Hametuha\\RestPatternTest": "tests/src"
        }
    },
    "require": {
        "php": "^5.6|^7.0|^8",
        "hametuha/singleton-pattern": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6|^7",
        "yoast/phpunit-polyfills": "^1.0"
    }
}
